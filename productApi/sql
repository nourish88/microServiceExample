CREATE TABLE [dbo].[Products] (
    [Id]              INT            IDENTITY (1, 1) NOT NULL,
    [Name]            NVARCHAR (MAX) NOT NULL,
    [Description]     NVARCHAR (MAX) NOT NULL,
    [Quantity]        INT            NOT NULL,
    [Price]           INT            NOT NULL,
    [Discount]        INT            NOT NULL,
    [CreatedDateTime] NVARCHAR (14)  NOT NULL,
    [Status]          INT            NOT NULL
);

